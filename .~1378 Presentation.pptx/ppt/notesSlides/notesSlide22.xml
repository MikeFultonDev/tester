              </a:rPr>
              <a:t>Enabling AI on </a:t>
            </a:r>
            <a:r>
              <a:rPr lang="en-US">
                <a:solidFill>
                  <a:schemeClr val="bg1">
                    <a:lumMod val="95000"/>
