            </a:r>
            <a:r>
              <a:rPr kumimoji="0" lang="en-US" b="0" i="0" u="none" strike="noStrike" kern="1200" cap="none" spc="0" normalizeH="0" baseline="30000" noProof="0" dirty="0">
