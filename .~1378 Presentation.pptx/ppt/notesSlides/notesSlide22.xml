                  </a:schemeClr>
                </a:solidFill>
              </a:rPr>
              <a:t>and IBM</a:t>
            </a:r>
            <a:r>
              <a:rPr lang="en-US" altLang="en-US" baseline="30000" dirty="0">
                <a:solidFill>
                  <a:schemeClr val="bg1">
