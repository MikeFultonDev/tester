                </a:solidFill>
              </a:rPr>
              <a:t>IBM Z </a:t>
            </a:r>
            <a:r>
              <a:rPr lang="en-US" dirty="0">
                <a:solidFill>
                  <a:schemeClr val="bg1">
                    <a:lumMod val="95000"/>
