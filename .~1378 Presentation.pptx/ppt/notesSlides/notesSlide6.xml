              <a:buNone/>
              <a:tabLst/>
              <a:defRPr/>
            </a:pPr>
            <a:endParaRPr kumimoji="0" lang="en-US" sz="1000" b="0" i="0" u="none" strike="noStrike" kern="1200" cap="none" spc="0" normalizeH="0" baseline="0" noProof="0" dirty="0">
