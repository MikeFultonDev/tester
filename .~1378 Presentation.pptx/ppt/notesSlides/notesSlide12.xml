                </a:schemeClr>
              </a:solidFill>
              <a:latin typeface="IBM Plex Sans Light" panose="020B0403050000000000" pitchFamily="34" charset="0"/>
            </a:endParaRPr>
          </a:p>
          <a:p>
            <a:pPr defTabSz="914400">
              <a:lnSpc>
                <a:spcPct val="100000"/>
              </a:lnSpc>
              <a:spcBef>
                <a:spcPts val="600"/>
              </a:spcBef>
              <a:defRPr sz="1000">
