              <a:t>Automate Z Tasks: </a:t>
            </a:r>
          </a:p>
          <a:p>
            <a:pPr marL="0" marR="0" lvl="0" indent="0" algn="l" defTabSz="914400" rtl="0" eaLnBrk="1" fontAlgn="auto" latinLnBrk="0" hangingPunct="1">
              <a:lnSpc>
                <a:spcPct val="100000"/>
